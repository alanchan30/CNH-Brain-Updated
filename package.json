{
  "name": "fastapi-vite-app",
  "version": "1.0.0",
  "description": "Full-stack app with FastAPI backend and Vite frontend",
  "scripts": {
    "setup": "npm run setup:backend && npm run setup:frontend",
    "setup:backend": "cd backend && pip install -r requirements.txt",
    "setup:frontend": "cd frontend && npm install",
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:backend": "cd backend && uvicorn main:app --reload --host 0.0.0.0 --port 8000",
    "start:frontend": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build",
    "serve": "cd frontend && npm run preview"
  },
  "keywords": [
    "fastapi",
    "vite",
    "fullstack"
  ],
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.49.1",
    "axios": "^1.8.4",
    "dotenv": "^16.4.7"
  }
}